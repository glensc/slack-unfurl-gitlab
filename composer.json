{
    "name": "glen/slack-unfurl-gitlab",
    "description": "GitLab links unfurler for slack-unfurl",
    "license": "MIT",
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "GitlabSlackUnfurl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GitlabSlackUnfurl\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "m4tthumphrey/php-gitlab-api": "^11",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "conflict": {
        "glen/slack-unfurl": "<0.9.0"
    },
    "require-dev": {
        "glen/slack-unfurl": "^0.9.0",
        "ocramius/lazy-property": "^1.0|^2.0",
        "phpunit/phpunit": "^9.5",
        "symfony/dotenv": "^6.1",
        "symfony/var-dumper": "^6.1",
        "symfony/yaml": "^6.1"
    }
}
